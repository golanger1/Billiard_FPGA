//////////--------------------------------------------------------------------------------------------------------------= 
			
//logic not_Transparent; //omer 31.08

			
 // pipeline (ff) to get the pixel color from the array 	 
//////////--------------------------------------------------------------------------------------------------------------= 
always_ff@(posedge clk or negedge resetN) 
begin 
	if(!resetN) begin 
		RGBout <=	TRANSPARENT_ENCODING; 
		//not_Transparent = 1'b0; //omer 31.08
		Ghole_DR_VEC = {(`NUM_HOLES){1'b0}};
	end 
	else 
		begin 
			RGBout <= TRANSPARENT_ENCODING ; // default  
			//not_Transparent = 1'b0; //omer 31.08
			Ghole_DR_VEC = {(`NUM_HOLES){1'b0}};
			
			for ( unsigned int i = 1; i < NUM_HOLES + 1; i++)
				begin
					if (InsideRectangle[i]) 
						begin							
							if(object_colors[offsetY[i]][offsetX[i]] == HOLE_ENCODING)
								begin
								
									if ( i == requested_hole )
										begin
											RGBout <= REQUEST_COLOR; 
										end
									else
										begin
											RGBout <= 8'h00;
										end
										
									Ghole_DR_VEC[i] = 1'b1;
									Hole_ID <= i[2:0];
								end
							else
								begin
									RGBout <= TRANSPARENT_ENCODING;
								end
						end
				end
				
				
			/**
					if (InsideRectangle[1]) //   1 - BlueBall
						begin
					//if(object_colors[offsetY[1]][offsetX[1]] == GENERIC_COLOR_ENCODING || object_colors[offsetY[1]][offsetX[1]] == 1'b0)
						//begin
							//not_Transparent = 1'b1;
							
							if(object_colors[offsetY[1]][offsetX[1]] == HOLE_ENCODING)
								begin
									RGBout <= 8'h00;
									Ghole_DR_VEC[1] = 1'b1;
								end
							else
								begin
									RGBout <= TRANSPARENT_ENCODING;
								end
						//end
						end ***/
			/***	
			if (InsideRectangle[2] && balls_in_game[2]) //   2 - GreenBall
				begin
					if(object_colors[offsetY[2]][offsetX[2]] == GENERIC_COLOR_ENCODING || object_colors[offsetY[2]][offsetX[2]] == 1'b0)
						begin
							not_Transparent = 1'b1;
							Ghole_DR_VEC[2] = 1'b1;
							HitEdgeCode[2] <= hit_colors[offsetY[2] >> 3][offsetX[2] >> 3 ]; // get hitting edge from the colors table
							if(object_colors[offsetY[2]][offsetX[2]] == GENERIC_COLOR_ENCODING)
								begin
									RGBout <= balls_colors[2];
								end
							else
								begin
									RGBout <= 1'b0;
								end						
						end
				end
				
			if (InsideRectangle[0] && balls_in_game[0]) //   0 - WhiteBall
				begin
					if(object_colors[offsetY[0]][offsetX[0]] == GENERIC_COLOR_ENCODING || object_colors[offsetY[0]][offsetX[0]] == 1'b0)
						begin
							not_Transparent = 1'b1;
							Ghole_DR_VEC[0] = 1'b1;
							HitEdgeCode[0] <= hit_colors[offsetY[0] >> 3][offsetX[0] >> 3 ]; // get hitting edge from the colors table
							if(object_colors[offsetY[0]][offsetX[0]] == GENERIC_COLOR_ENCODING)
								begin
									RGBout <= balls_colors[0];
								end
							else
								begin
									RGBout <= 1'b0;
								end
						end
				end
			***/
/***			
			if (not_Transparent == 1'b0)
				begin
					RGBout <= TRANSPARENT_ENCODING; // originally: object_colors[offsetY][offsetX];
				end
***/	 	 
		end 
end 
 
//////////--------------------------------------------------------------------------------------------------------------= 
// decide if to draw the pixel or not 
assign Ghole_DR = (RGBout != TRANSPARENT_ENCODING ) ? 1'b1 : 1'b0 ; // get optional transparent command from the bitmpap   
//assign Ghole_DR_VEC = InsideRectangle & balls_in_game & ;
endmodule 
